{
    "name": "vasildakov/maxoptra",
    "description": "Maxoptra REST API Client",
    "keywords": ["maxoptra", "rest", "api", "client", "php"],
    "authors": [
        {
            "name": "Vasil Dakov",
            "email": "vasildakov@gmail.com"
        }
    ],
    "require": {
    	"php": ">=5.4.0",
        "zendframework/zend-config": "2.6.*",
        "guzzlehttp/guzzle": "6.1.*",
        "guzzlehttp/guzzle-services": "dev-guzzle6",
        "guzzlehttp/psr7": "1.2.*",
        "doctrine/collections": "^1.3"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.0",
        "phpunit/phpunit-mock-objects": "3.0.*",
        "behat/behat": "~3.0",
        "phpspec/phpspec": "~2.1"
    },
    "autoload": {
        "psr-4": {
            "VasilDakov\\MaxOptra\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "VasilDakov\\MaxOptra\\Tests\\": "tests/"
        }
    }
}
