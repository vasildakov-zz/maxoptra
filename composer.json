{
    "name": "vasildakov/maxoptra",
    "description": "Maxoptra REST API Client",
    "keywords": ["maxoptra", "rest", "api", "client", "php"],
    "authors": [
        {
            "name": "Vasil Dakov",
            "email": "vasildakov@gmail.com"
        }
    ],
    "require": {
    	"php": ">=5.3.3",
        "guzzlehttp/guzzle": "5.3.*",
        "guzzlehttp/guzzle-services": "0.5.*",
        "zendframework/zend-config": "2.6.*"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "phpunit/phpunit-mock-objects": "2.3.0",
        "behat/behat": "~3.0",
        "phpspec/phpspec": "~2.1"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "VasilDakov\\MaxOptra\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "VasilDakov\\MaxOptra\\Tests\\": "tests/"
        }
    }
}
